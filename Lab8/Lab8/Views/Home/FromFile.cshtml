@model List<Fraction>
@{
    ViewData["Title"] = "From File";
}

<form method="get">
    <label>Path:</label>
    <input type="file" name="src" accept=".txt" />
    <input type="submit" value="Submit" />
</form>

@if (Model != null)
{
    for (int i = 0; i < Model.Count; i++)
    {
        <p>@Model[i].Numerator / @Model[i].Denominator</p>
    }
}